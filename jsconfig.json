{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "boot/*": [
        "src/boot/*"
      ],
      "vue$": [
        "node_modules/vue/dist/vue.esm.js"
      ],
      "helpers/*": ["src/helpers/*"],
      "mixins/*": ["src/mixins/*"],
      "services/*": ["src/services/*"],
      "store/*": ["src/store/*"]
    }
  },
  "exclude": [
    "dist",
    ".quasar",
    "node_modules"
  ]
}
